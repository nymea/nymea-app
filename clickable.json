{
  "builder": "qmake",
  "kill": "nymea-app",
  "build_args": "CONFIG+=ubports",
  "dependencies_target": [
    "libavahi-client-dev",
    "libavahi-common-dev",
    "libqt5charts5-dev",
    "qt5-default",
    "qml-module-qtcharts",
    "qml-module-qt-labs-calendar",
    "libconnectivity-qt1-dev",
    "libunity-api-dev"
  ],
  "install_lib": [
    "/usr/lib/${ARCH_TRIPLET}/libQt5Nfc.so*"
  ],
  "install_qml": [
    "/usr/lib/${ARCH_TRIPLET}/qt5/qml/Qt/labs/calendar/"
  ]
}
